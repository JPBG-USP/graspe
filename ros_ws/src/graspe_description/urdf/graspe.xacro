<?xml version="1.0" ?>
<robot name="graspe" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:arg name="is_ignition" default="true"/>

<xacro:include filename="$(find graspe_description)/urdf/materials.xacro" />
<xacro:include filename="$(find graspe_description)/urdf/graspe.trans" />
<xacro:include filename="$(find graspe_description)/urdf/graspe.gazebo" />

<!-- Theta1 -->
<xacro:property name="ajuste1" value="0.04"/>
<xacro:property name="ajuste2" value="0.083"/>
<xacro:property name="ajuste3" value="0.0405"/>
<xacro:property name="ajuste4" value="0.128004"/>
<xacro:property name="ajuste5" value="0.05"/>
<xacro:property name="ajuste6" value="0.01"/>
<xacro:property name="ajuste7" value="0.025004"/>
<xacro:property name="ajuste8" value="${0.2105 + 0.158 - ajuste4 }"/>
<xacro:property name="ajuste9" value="0.015"/>

<link name="base_link">
  <inertial>
    <origin xyz="-3.793254127438435e-18 -3.793254127438435e-18 0.026500000000000003" rpy="0 0 0"/>
    <mass value="1.1763536611734302"/>
    <inertia ixx="0.00054" iyy="0.00054" izz="0.000529" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="revolution_link_1">
  <inertial>
    <origin xyz="0.0 0.0 0.049999999999999996" rpy="0 0 0"/>
    <mass value="0.9864600932271952"/>
    <inertia ixx="0.000921" iyy="0.000921" izz="0.000197" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.0 0.0 -0.053" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/revolution_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 -0.053" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/revolution_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="theta1" type="continuous">
  <origin xyz="0.0 0.0 0.053" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="revolution_link_1"/>
  <axis xyz="0.0 0.0 1.0"/>
</joint>

<!-- Fixed jointJoint -->
<link name="fixed_link_1">
  <inertial>
    <origin xyz="0.0 -0.017499999999999998 ${-0.04083760550324669 + ajuste1}" rpy="0 0 0"/>
    <mass value="0.1984169956244406"/>
    <inertia ixx="5e-05" iyy="1.6e-05" izz="4.9e-05" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 0.0 ${-0.206 + ajuste1}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/fixed_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 0.0 ${-0.206 + ajuste1}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/fixed_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="rigid1" type="fixed">
  <origin xyz="0.0 0.0 ${0.153 - ajuste1}" rpy="0 0 0"/>
  <parent link="revolution_link_1"/>
  <child link="fixed_link_1"/>
</joint>


<!-- Theta2 -->
<link name="revolution2_link_1">
  <inertial>
    <origin xyz="4.126627129485931e-08 ${0.12800435621809492 - ajuste4} ${0.0647561892724049 - ajuste2 + ajuste3}" rpy="0 0 0"/>
    <mass value="0.06955361957457033"/>
    <inertia ixx="1.6e-05" iyy="1.8e-05" izz="1.8e-05" ixy="-0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.0 ${0.158 - ajuste4} ${-0.123 - ajuste2 + ajuste3}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/revolution2_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.0 ${0.158 - ajuste4} ${-0.123 - ajuste2 + ajuste3}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/revolution2_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="theta2" type="revolute">
  <origin xyz="-0.0 ${-0.158 + ajuste4} ${-0.083 + ajuste2}" rpy="0 0 0"/>
  <parent link="fixed_link_1"/>
  <child link="revolution2_link_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
  <limit upper="0.785398" lower="-0.785398" effort="100" velocity="100"/>
</joint>

<!-- d3 -->

<link name="slider_link_1">
  <inertial>
    <origin xyz="0.0 3.562180949066851e-07 0.01999999999999999" rpy="0 0 0"/>
    <mass value="0.22195352097611884"/>
    <inertia ixx="4.2e-05" iyy="4.2e-05" izz="2.5e-05" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 0.029996 -0.2055" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/slider_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 0.029996 -0.2055" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/slider_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="d3" type="prismatic">
  <origin xyz="0.0 ${0.128004 - ajuste4} ${0.0825 - ajuste5}" rpy="0 0 0"/>
  <parent link="revolution2_link_1"/>
  <child link="slider_link_1"/>
  <axis xyz="0.0 0.0 1.0"/>
  <limit upper="0.0" lower="-0.026" effort="100" velocity="100"/>
</joint>

<!-- fixed -->
<link name="fixed2_link_1">
  <inertial>
    <origin xyz="0.0 0.015000356218094904 ${0.00964996017692743 - ajuste6}" rpy="0 0 0"/>
    <mass value="0.15384250264568528"/>
    <inertia ixx="5e-05" iyy="8e-06" izz="5e-05" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.0 0.029996 ${-0.2455 - ajuste6}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/fixed2_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.0 0.029996 ${-0.2455 - ajuste6}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/fixed2_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="rigid2" type="fixed">
  <origin xyz="-0.0 0.0 ${0.04 + ajuste6}" rpy="0 0 0"/>
  <parent link="slider_link_1"/>
  <child link="fixed2_link_1"/>
</joint>

<!-- Theta4 -->

<link name="endeffector_link_1">
  <inertial>
    <origin xyz="-3.206969956259798e-10 ${0.24049635982298995 - ajuste8} ${-0.002508226805447933 + ajuste9}" rpy="0 0 0"/>
    <mass value="0.1076530467419571"/>
    <inertia ixx="1.1e-05" iyy="1.3e-05" izz="1.8e-05" ixy="-0.0" iyz="-0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 ${0.240496 - ajuste8} ${-0.270504 + ajuste9}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/endeffector_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 ${0.240496 - ajuste8} ${-0.270504 + ajuste9}" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find graspe_description)/meshes/endeffector_link_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="theta4" type="revolute">
  <origin xyz="0.0 ${-0.2105 + ajuste8} ${0.025004 - ajuste7}" rpy="0 0 0"/>
  <parent link="fixed2_link_1"/>
  <child link="endeffector_link_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
  <limit upper="0.698132" lower="-0.698132" effort="100" velocity="100"/>
</joint>


</robot>
